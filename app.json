{
  "expo": {
    "name": "prodribbler",
    "slug": "prodribbler",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alliancetech.prodribbler",
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to your camera so you can take photos to personalize your player card in the football training app. Your photos will only be used within the app.",
        "NSPhotoLibraryUsageDescription": "We need access to your photo library so you can select photos to customize your player card in the football training app. Your photos will only be used within the app."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.alliancetech.prodribbler"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e20d50bd-72a3-4b57-9504-990ae037cdb3"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/e20d50bd-72a3-4b57-9504-990ae037cdb3"
    }
  }
}
